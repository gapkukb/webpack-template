{
  "env": {
    "browser": true,
    "es2020": true
  },
  "overrides": [],
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn"
  }
}
